; vim: set ts=8 sw=8 sts=8 tw=78 noet :
[user]
	email = ketheridge@campusexplorer.com
	name = Karen Etheridge
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = true
[color "diff"]
[push]
	default = upstream
[branch]
	autosetupmerge = true
	autosetuprebase = always

[diff "gz"]
	textconv = zcat
	cachetextconv = true
	binary = true

;[column]
;	branch = auto

; see doc/howto/gitflow.txt
[gitflow "branch"]
	master = production
	develop = trunk
[gitflow "prefix"]
	feature = feature/
	release = staging/
	hotfix = hotfix/
	support = support/
	cepatch = cepatch/
	versiontag = release

[core]
	excludesfile = /home/vagrant/.gitexclude

